@using EntityLayer.Concrete
@model List<Category>
@{
    Layout = null;
}

<table>
    <tr>
    <th>Kategori Adı</th>
    <th>Kategori Açıklaması</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
            <td>@item.CategoryDescription</td>
        </tr>
    }

</table>